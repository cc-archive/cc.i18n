#: checkouts/cc.engine/cc/engine/templates/catalog_pages/licenses-index.html:163
msgid "Three “Layers” Of Licenses"
msgstr ""

#: checkouts/cc.engine/cc/engine/templates/catalog_pages/licenses-index.html:175
msgid "Machine Readable"
msgstr ""

#: checkouts/cc.engine/cc/engine/templates/catalog_pages/licenses-index.html:178
msgid "Human Readable"
